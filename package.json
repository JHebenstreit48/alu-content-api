{
  "name": "asphalt-content-api",
  "version": "1.0.0",
  "description": "Database for Asphalt Legends Unite Tracker other content",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "dev": "ts-node -r tsconfig-paths/register src/server.ts",
    "seed:blueprints": "ts-node -r tsconfig-paths/register src/scripts/importBlueprintPrices.ts",
    "seed:manufacturers": "ts-node -r tsconfig-paths/register src/scripts/importManufacturers.ts",
    "seed:garagelevels": "ts-node -r tsconfig-paths/register src/scripts/importGarageLevels.ts",
    "seed:all": "npm run seed:blueprints && npm run seed:manufacturers && npm run seed:garagelevels",
    "ts": "ts-node",
    "build": "tsc",
    "start:watch": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "start:dev": "npm run start:watch"
  },
  "author": "Justin Hebenstreit",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.18",
    "cross-env": "^10.1.0",
    "dotenv": "^16.4.5",
    "inquirer": "^8.2.4",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.4"
  }
}
